{
	"name": "@vscode/extension-telemetry",
	"version": "0.0.1",
	"private": false,
	"description": "A module for Visual Studio Code extensions to report consistent telemetry.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"sideEffects": false,
	"type": "module",
	"main": "./dist/node/node/telemetryReporter.js",
	"browser": "./dist/browser/browser/telemetryReporter.js",
	"types": "./dist/telemetryReporter.d.ts",
	"scripts": {
		"build": "npm run compile",
		"compile": "tsc -p src/browser/tsconfig.json && tsc -p src/node/tsconfig.json",
		"prepack": "node injectVersion.js",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"dependencies": {
		"@microsoft/1ds-core-js": "4.1.1",
		"@microsoft/1ds-post-js": "4.1.1",
		"@microsoft/applicationinsights-web-basic": "3.1.1"
	},
	"devDependencies": {
		"@types/mocha": "10.0.6",
		"@types/node": "20.11.30",
		"@types/sinon": "17.0.3",
		"@types/vscode": "1.87.0",
		"mocha": "10.3.0",
		"sinon": "17.0.1",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5",
		"user-agent-data-types": "0.4.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
